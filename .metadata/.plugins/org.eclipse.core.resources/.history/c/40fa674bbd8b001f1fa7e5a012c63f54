package assignment;

public class DoubleLinkedList {
	Node root,last;
	
	void create_List() {
		root=last=null;
	}
	
	void right_Insert(int data)
	{
		Node newNode =new  Node(data);
		if(root==null)
		{
			root=last=newNode;
			last.next=root;
		}
		else
		{
			last.next = newNode;
			last = newNode;
			last.next=root;
		}
		System.out.println(data + " inserted on the right");
	}

}
