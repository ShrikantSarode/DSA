package sorting;

public class Sorting_Main {

	static void print_array(int a[]) {
		System.out.println("Array Has:");
		for (int i = 0; i < a.length; i++) {
			System.out.print(a[i] + " , ");
		}
	}

	public static void main(String[] args) {

		int a[] = { 77, 88, 44, 99, 33, 55, 66, 22, 11 };
		print_array(a);

	}
}
